Class {
	#name : #BrGlamorousLabelTheme,
	#superclass : #Object,
	#instVars : [
		'textColor'
	],
	#category : #'Brick-Theme-Glamorous'
}

{ #category : #accessing }
BrGlamorousLabelTheme >> textColor [
	^ textColor
]

{ #category : #accessing }
BrGlamorousLabelTheme >> textColor: anObject [
	textColor := anObject
]
